#include<bits/stdc++.h>
using namespace std;

void printBinary(int num){
    for(int i=10; i>=0; i--){
        cout<<((num>>i)&1);
    }
    cout<<endl;
}

int main(){
    
    int n=15;
    if(n & (n-1)){
        cout<<"Not power of two\n";
    }
    else{
        cout<<"Power of two\n";
    }

    return 0;
}
